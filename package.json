{
  "name": "pattern-lib-docs",
  "version": "0.0.1",
  "description": "Documentation for pattern library",
  "scripts": {
    "prebuild": "rimraf '{dist, lib}' && yarn lint",
    "build": "yarn webpack",
    "start": "webpack-dev-server --hot --mode development --env.env=dev",
    "build:dev": "yarn build -- --mode development --env.env=dev",
    "build:prod": "yarn build -- --mode production --env.env=prod",
    "build:lib": "yarn lint && babel ./src --out-dir ./lib",
    "build:docs": "start-storybook -s ./assets -p 8080 -c .storybook --ci",
    "watch:dev": "yarn build:dev --watch",
    "watch:prod": "yarn build:prod --watch",
    "lint": "eslint 'src/**/*.js'",
    "predeploy": "yarn prebuild",
    "postdeploy": "cp -R assets/. dist/assets",
    "deploy": "build-storybook -c .storybook -o dist"
  },
  "keywords": [
    "front-end",
    "React",
    "Javascript",
    "pattern-library"
  ],
  "author": "Obie",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "7.3.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-react-jsx": "7.3.0",
    "@babel/preset-env": "7.3.4",
    "@storybook/addon-options": "5.0.0",
    "@storybook/addon-storysource": "5.0.0",
    "@storybook/addon-viewport": "5.0.0",
    "@storybook/components": "5.0.0",
    "@storybook/theming": "5.0.0",
    "autoprefixer": "9.4.10",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "compression-webpack-plugin": "2.0.0",
    "css-loader": "2.1.0",
    "eslint": "5.15.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-react": "7.12.4",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "4.6.0",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-color-function": "4.0.1",
    "postcss-css-variables": "0.12.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "terser-webpack-plugin": "1.2.3",
    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "@0bie/pattern-lib": "0.0.1",
    "@0bie/pattern-lib-react": "1.0.0",
    "@storybook/react": "5.0.0",
    "babel-polyfill": "6.26.0",
    "react": "16.8.4",
    "react-dom": "16.8.4"
  }
}
